{
  "name": "diary",
  "version": "1.0.0",
  "description": "a light-weight diary application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "project_setup": "concurrently \"npm run install_client\" \"npm run install_server\" ",
    "install_root": "npm install",
    "install_client": "cd client && npm install",
    "install_server": "cd server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mek0124/diary-desktop.git"
  },
